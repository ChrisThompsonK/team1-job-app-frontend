{
  "name": "team1-job-app-frontend",
  "version": "1.0.0",
  "description": "Modern Node.js TypeScript project with ES modules",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "keywords": [
    "typescript",
    "nodejs",
    "es-modules"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/express": "^5.0.3",
    "@types/node": "^20.8.0",
    "@types/nunjucks": "^3.2.6",
    "@vitest/coverage-v8": "^3.2.4",
    "daisyui": "^5.1.26",
    "tsx": "^4.20.6",
    "typescript": "^5.2.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "express": "^5.1.0",
    "tailwindcss": "^4.1.13",
    "nunjucks": "^3.2.4"

  }
}
